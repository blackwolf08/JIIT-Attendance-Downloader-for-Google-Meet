{
  "name": "Attendance Downloader for Google Meet - JIIT",
  "version": "1.0",
  "description": "Download attendance on a click of a button",
  "manifest_version": 2,
  "permissions": ["storage", "activeTab", "declarativeContent"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": { "128": "icon.png" },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
